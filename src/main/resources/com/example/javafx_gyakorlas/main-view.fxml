<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="483.0" prefWidth="825.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_gyakorlas.MainController">
   <children>
      <HBox minHeight="30.0" prefHeight="30.0" spacing="10.0">
         <children>
            <Button maxWidth="100.0" minWidth="100.0" mnemonicParsing="false" onAction="#onHozzaadasButtonClick" prefWidth="100.0" text="Hozzáadás" />
            <Button mnemonicParsing="false" onAction="#onModositasButtonClick" text="Módosítás" />
            <Button mnemonicParsing="false" onAction="#onTorlesButtonClick" text="Törlés" />
         </children>
      </HBox>
      <TableView fx:id="filmTable" prefHeight="412.0" prefWidth="785.0">
        <columns>
          <TableColumn fx:id="colSorszam" prefWidth="75.0" text="Sorszám" />
          <TableColumn fx:id="colKodszam" prefWidth="75.0" text="Kódszám" />
            <TableColumn fx:id="colNev" prefWidth="75.0" text="Név" />
            <TableColumn fx:id="colAr" prefWidth="75.0" text="Ár" />
            <TableColumn fx:id="colMennyiseg" prefWidth="75.0" text="Mennyiség" />
            <TableColumn fx:id="colKepUrl" prefWidth="75.0" text="Kép url" />
            <TableColumn fx:id="colKategoria" prefWidth="75.0" text="Kategória" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
